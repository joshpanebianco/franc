<%= simple_form_for @application do |f| %>
  <%= f.input :first_name, placeholder: 'Daniel' %>
  <%= f.input :last_name, placeholder: 'Bennetts' %>
  <%= f.input :email, placeholder: 'dan@example.com' %>
  <%= f.input :videoURL, placeholder: 'http://example.com' %>
  <%= f.input :comments, placeholder: 'You comment here' %>
  <%= f.button :submit %>
  <% end %>
